{
  "name": "k8sapp_ui_template",
  "description": "This is the GitHub template repo for all Next.js UIs",
  "author": "Andrews McMeel Universal",
  "homepage": "https://github.com/Andrews-McMeel-Universal/k8sapp_ui_template",
  "version": "0.0.0",
  "license": "UNLICENSED",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/Andrews-McMeel-Universal/k8sapp_ui_template"
  },
  "scripts": {
    "build": "next build",
    "postbuild": "yarn build && next-sitemap --config next-sitemap.config.js",
    "build:analyze": "ANALYZE=true yarn build",
    "dev": "next dev",
    "docker:build": "docker compose up --build",
    "docker:run": "docker compose up",
    "format": "prettier . --list-different --write",
    "keys:edit": "pwsh -c \"./Edit-Secrets.ps1\"",
    "keys:get": "pwsh -c \"./Get-Secrets.ps1 -Environment ${1}\"",
    "keys:save": "pwsh -c \"./Set-Secrets.ps1\"",
    "keys:setup": "run-s \"keys:get {2:-development}\" -- ${2}",
    "lint:container": "npx mega-linter-runner",
    "lint:js": "eslint \"./{src,playwright}/**/*.{js,jsx}\" --fix",
    "lint:js:check": "npx eslint-config-prettier ${1}",
    "lint:js:rules": "eslint --print-config .eslintrc.js",
    "lint:styles": "stylelint **/*.scss --fix",
    "lint:styles:check": "stylelint-config-prettier-scss-check",
    "lint:styles:nofix": "stylelint **/*.scss",
    "lint:styles:rules": "stylelint --print-config .",
    "preflight": "yarn run lint:js && yarn run lint:styles && yarn run format && yarn run test:unit:ci && yarn run build && yarn run test:integration:ci && yarn run test:size && yarn run test:integration:report",
    "pretest:integration:ci": "npx playwright install --with-deps",
    "setup": "corepack enable && yarn install && yarn run keys:get ${0:-development}",
    "start": "next start",
    "storybook:build": "storybook build",
    "storybook:dev": "storybook dev -p 6006",
    "storybook:dev:debug": "storybook dev -p 6006 --debug-webpack",
    "test:dependencies": "npm-check ./ --specials=bin,nextjs,react,webpack,jest,babel,eslint,prettier,stylelint,typescript,node,npm,yarn --no-color",
    "test:integration": "playwright test ${0:-.} --trace on",
    "test:integration:ci": "CI=true yarn pretest:integration:ci && playwright test",
    "test:integration:debug": "playwright test ${0:-.} --debug --trace on",
    "test:integration:record": "playwright codegen",
    "test:integration:report": "playwright show-report",
    "test:size": "size-limit",
    "test:unit": "jest ${0:-.} --verbose",
    "test:unit:ci": "jest ${0:-.} --ci",
    "test:unit:watch": "jest --watch --detectOpenHandles"
  },
  "dependencies": {
    "@next/bundle-analyzer": "^13.4.19",
    "axe-playwright": "^1.2.3",
    "classnames": "^2.3.2",
    "console-stamp": "^3.1.2",
    "date-fns": "^2.30.0",
    "next": "^14.0.3",
    "next-auth": "^4.24.5",
    "next-sitemap": "^4.2.3",
    "prop-types": "^15.8.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-gtm-module": "^2.0.11",
    "sass": "^1.69.5",
    "sharp": "^0.32.6"
  },
  "devDependencies": {
    "@axe-core/react": "^4.8.1",
    "@babel/core": "^7.21.8",
    "@babel/eslint-parser": "^7.23.3",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-react": "^7.23.3",
    "@ianvs/prettier-plugin-sort-imports": "^4.1.1",
    "@playwright/test": "^1.34.3",
    "@size-limit/file": "^8.2.4",
    "@storybook/addon-a11y": "^7.0.17",
    "@storybook/addon-essentials": "^7.0.17",
    "@storybook/addon-interactions": "^7.0.17",
    "@storybook/addon-links": "^7.0.17",
    "@storybook/blocks": "^7.0.17",
    "@storybook/nextjs": "^7.0.17",
    "@storybook/react": "^7.0.17",
    "@storybook/test-runner": "^0.15.2",
    "@storybook/testing-library": "^0.0.14-next.2",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "babel-jest": "^29.5.0",
    "babel-loader": "^9.1.2",
    "babel-plugin-dynamic-import-node": "^2.3.3",
    "chromatic": "^9.1.0",
    "eslint": "8.41.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-next": "13.4.3",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-chai-friendly": "^0.7.2",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.6.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-storybook": "^0.6.12",
    "eslint-plugin-unused-imports": "^3.0.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "jest-fetch-mock": "^3.0.3",
    "npm-check": "^6.0.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.31",
    "prettier": "^2.8.8",
    "size-limit": "^8.2.4",
    "storybook": "^7.0.17",
    "stylelint": "^15.6.2",
    "stylelint-a11y": "^1.2.3",
    "stylelint-color-format": "^1.1.0",
    "stylelint-config-prettier-scss": "^1.0.0",
    "stylelint-config-recess-order": "^4.0.0",
    "stylelint-config-standard-scss": "^9.0.0",
    "stylelint-no-unsupported-browser-features": "^6.1.0",
    "typescript": "^5.3.2"
  },
  "packageManager": "yarn@3.6.1"
}
