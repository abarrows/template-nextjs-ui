{
  "name": "k8sapp_ui_template",
  "description": "This is the GitHub template repo for all Next.js UIs",
  "author": "Andrews McMeel Universal",
  "homepage": "https://github.com/Andrews-McMeel-Universal/k8sapp_ui_template",
  "version": "0.0.0",
  "license": "UNLICENSED",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/Andrews-McMeel-Universal/k8sapp_ui_template"
  },
  "scripts": {
    "keys:get": "pwsh -c \"./Get-Secrets.ps1 -Environment ${1}\"",
    "keys:edit": "pwsh -c \"./Manage-SecretsFile.ps1 -Decrypt\"",
    "keys:save": "pwsh -c \"./Manage-SecretsFile.ps1 -Encrypt && ./Set-Secrets.ps1\"",
    "setup": "yarn install && yarn keys:get ${1}",
    "dev": "next dev",
    "build": "next build",
    "postbuild": "next-sitemap",
    "build:analyze": "ANALYZE=true yarn build",
    "start": "next start",
    "docker-compose:build": "docker compose up --build ui",
    "docker-compose:run": "docker compose up",
    "docker:build": "docker build -t k8sapp_ui_template .",
    "docker:run": "docker run --rm -it -p 3000:3000 k8sapp_ui_template",
    "lint:container": "npx mega-linter-runner",
    "lint:js": "eslint \"./{src,playwright}/**/*.{js,jsx}\" --fix",
    "lint:js:rules": "eslint --print-config .eslintrc.js",
    "lint:styles": "stylelint **/*.scss --fix",
    "lint:styles:rules": "stylelint --print-config .",
    "lint:styles:nofix": "stylelint **/*.scss",
    "format": "prettier --write .",
    "pretest:integration:ci": "npx playwright install --with-deps",
    "test:integration": "playwright test --trace on",
    "test:integration:ci": "CI=true playwright test",
    "test:integration:record": "playwright codegen",
    "test:integration:report": "playwright show-report",
    "test:links": "start-server-and-test start http://localhost:3000 \"blc --input http://localhost:3000 --filter-level 3 -ro --exclude /pages/dynamic-sitemap.xml --exclude sitemap.xml.js --exclude https://fonts.* --verbose \"",
    "test:size": "size-limit",
    "test:unit": "jest src --verbose",
    "test:unit:ci": "jest src --ci",
    "test:unit:report": "jest",
    "test:unit:watch": "jest --watch --detectOpenHandles",
    "preflight": "yarn browserslist --update-db && yarn lint:js && yarn lint:styles && yarn format-all && yarn test:unit:ci && yarn build && yarn test:integration:ci && yarn test:size && yarn test:integration:report"
  },
  "dependencies": {
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@next/bundle-analyzer": "^13.0.2",
    "@sentry/integrations": "^7.47.0",
    "@sentry/nextjs": "^7.40.0",
    "@sentry/webpack-plugin": "^1.18.3",
    "babel-plugin-dynamic-import-node": "^2.3.3",
    "beautiful-react-hooks": "^3.12.2",
    "bootstrap": "^5.2.3",
    "broken-link-checker": "^0.7.8",
    "classnames": "^2.3.2",
    "console-stamp": "^3.1.1",
    "is_js": "^0.9.0",
    "next": "13.2.4",
    "next-auth": "^4.22.0",
    "next-seo": "^5.15.0",
    "next-sitemap": "^3.1.22",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-bootstrap-icons": "^1.10.2",
    "react-device-detect": "^2.2.3",
    "react-dom": "^18.2.0",
    "react-gtm-module": "^2.0.11",
    "react-html-parser": "^2.0.2",
    "react-responsive": "^9.0.2",
    "sass": "1.61.0",
    "sass-loader": "^13.2.0
    "swr": "2.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.21.0",
    "@babel/eslint-parser": "^7.21.3",
    "@babel/preset-typescript": "^7.21.0",
    "@playwright/test": "^1.32.2",
    "@size-limit/file": "^8.2.4",
    "@storybook/addon-essentials": "^7.0.2",
    "@storybook/addon-links": "^6.5.16",
    "@storybook/builder-webpack5": "^6.5.16",
    "@storybook/manager-webpack5": "^6.5.14",
    "@storybook/react": "^6.5.15",
    "@svgr/webpack": "^6.5.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/user-event": "14.4.3",
    "@trivago/prettier-plugin-sort-imports": "^3.4.0",
    "babel-jest": "^29.5.0",
    "babel-loader": "^9.1.2",
    "chromatic": "^6.17.1",
    "eslint": "^8.38.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-chai-friendly": "^0.7.2",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "mega-linter-runner": "^6.20.0",
    "mockdate": "^3.0.5",
    "postcss": "^8.4.21",
    "prettier": "^2.8.7",
    "react-test-renderer": "^18.2.0",
    "size-limit": "^8.2.4",
    "start-server-and-test": "2.0.0",
    "storybook-addon-grid": "^0.3.1",
    "storybook-addon-mock": "^3.2.0",
    "storybook-addon-next": "^1.7.3",
    "storybook-dark-mode": "^2.1.1",
    "stylelint": "^14.16.1",
    "stylelint-a11y": "^1.2.3",
    "stylelint-color-format": "^1.1.0",
    "stylelint-config-prettier-scss": "^0.0.1",
    "stylelint-config-recess-order": "^4.0.0",
    "stylelint-config-standard-scss": "^6.1.0",
    "stylelint-no-unsupported-browser-features": "^6.1.0"
  },
  "peerDependencies": {
    "postcss": "^8.4.19"
  }
}
