{
  "name": "k8sapp_ui_template",
  "version": "0.0.1",
  "description": "This is the GitHub template repo for all Next.js UIs",
  "author": "Andrews McMeel Universal",
  "homepage": "https://github.com/Andrews-McMeel-Universal/k8sapp_ui_template",
  "repository": {
    "type": "git",
    "url": "https://github.com/Andrews-McMeel-Universal/k8sapp_ui_template"
  },
  "license": "UNLICENSED",
  "private": true,
  "scripts": {
    "keys:decrypt": "pwsh -c \"./Manage-SecretsFile.ps1 -Decrypt\"",
    "keys:encrypt": "pwsh -c \"./Manage-SecretsFile.ps1 -Encrypt\"",
    "keys:get": "sh -c 'pwsh -c \"./Get-Secrets.ps1 -KeyVaultName ${0}\"'",
    "setup:os": "if [[ $OSTYPE == 'darwin'* ]]; then yarn config set packager \"brew\"; else yarn config set packager \"choco\"; fi $npm_config_packager install --cask powershell gpg-suite && pwsh -c \"Install-Module Az -Force\"; yarn install",
    "dev": "next",
    "build": "next build",
    "build:analyze": "ANALYZE=true yarn build",
    "postbuild": "next-sitemap",
    "start": "next start",
    "docker:build": "docker build -t k8sapp_ui_template .",
    "docker:run": "docker run --rm -it -p 3000:3000 k8sapp_ui_template",
    "test": "jest src --verbose",
    "test:ci": "jest src --ci",
    "test:links": "start-server-and-test start http://localhost:3000 \"blc --input http://localhost:3000 --filter-level 3 -ro --exclude /pages/dynamic-sitemap.xml --verbose \"",
    "test:watch": "jest --watch --detectOpenHandles",
    "lint:js": "eslint \"./{src,playwright}/**/*.{js,jsx}\" --fix",
    "lint:styles": "stylelint **/*.scss --fix",
    "lint:styles:nofix": "stylelint **/*.scss",
    "pw:test": "playwright test",
    "pw:test:ci": "CI=true yarn playwright test",
    "pw:test:trace": "playwright test --trace on",
    "format": "prettier --write .",
    "size": "size-limit",
    "storybook:build": "build-storybook",
    "storybook:run": "start-storybook -p 6006",
    "preflight": "yarn browserslist --update-db && yarn lint:js && yarn lint:styles && yarn format && yarn test && yarn build && yarn cy:ci"
  },
  "dependencies": {
    "@next/bundle-analyzer": "^13.0.2",
    "@sentry/integrations": "^6.0.3",
    "@sentry/nextjs": "^6.13.3",
    "@sentry/webpack-plugin": "^1.18.3",
    "beautiful-react-hooks": "^3.11.2",
    "classnames": "^2.3.2",
    "bootstrap": "^5.0.0",
    "broken-link-checker": "^0.7.8",
    "console-stamp": "^3.0.6",
    "is_js": "^0.9.0",
    "next": "^12.3.1",
    "next-auth": "^3.29.10",
    "next-seo": "^4.26.0",
    "next-sitemap": "^3.1.22",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-bootstrap-icons": "^1.10.2",
    "react-device-detect": "^2.0.1",
    "react-dom": "^18.2.0",
    "react-gtm-module": "^2.0.11",
    "react-html-parser": "^2.0.2",
    "react-responsive": "^8.2.0",
    "sass": "1.56.1",
    "sass-loader": "^13.2.0",
    "swr": "^1.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.21.0",
    "@babel/eslint-parser": "^7.19.1",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-react": "^7.18.6",
    "@playwright/test": "^1.31.1",
    "@size-limit/file": "^8.2.4",
    "@storybook/addon-essentials": "^6.4.22",
    "@storybook/addon-links": "^6.4.22",
    "@storybook/builder-webpack5": "^6.4.22",
    "@storybook/manager-webpack5": "^6.4.22",
    "@storybook/react": "^6.4.22",
    "@svgr/webpack": "^5.5.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/user-event": "14.4.3",
    "@trivago/prettier-plugin-sort-imports": "^3.4.0",
    "babel-jest": "^29.4.3",
    "babel-loader": "^9.1.2",
    "babel-plugin-dynamic-import-node": "^2.3.3",
    "chromatic": "^5.10.1",
    "eslint": "^8.27.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-chai-friendly": "^0.7.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.31.10",
    "eslint-plugin-react-hooks": "^4.6.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "29.3.1",
    "jest-environment-jsdom": "^29.3.1",
    "mockdate": "^3.0.5",
    "postcss": "^8.4.21",
    "prettier": "^2.8.1",
    "react-test-renderer": "^18.2.0",
    "size-limit": "^8.2.4",
    "start-server-and-test": "1.15.4",
    "storybook-addon-next": "^1.6.2",
    "stylelint": "^14.16.1",
    "stylelint-a11y": "^1.2.3",
    "stylelint-color-format": "^1.1.0",
    "stylelint-config-prettier-scss": "^0.0.1",
    "stylelint-config-recess-order": "^3.0.0",
    "stylelint-config-standard-scss": "^6.1.0",
    "stylelint-no-unsupported-browser-features": "^6.0.1"
  }
}
