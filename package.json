{
  "name": "k8sapp_ui_template",
  "version": "0.0.1",
  "description": "This is the GitHub template repo for all Next.js UIs",
  "author": "Andrews McMeel Universal",
  "homepage": "https://github.com/Andrews-McMeel-Universal/k8sapp_ui_template",
  "repository": {
    "type": "git",
    "url": "https://github.com/Andrews-McMeel-Universal/k8sapp_ui_template"
  },
  "license": "UNLICENSED",
  "private": true,
  "scripts": {
    "dev": "next",
    "build": "next build",
    "postbuild": "next-sitemap",
    "setup": "yarn install && yarn dev",
    "start": "next start",
    "docker:build": "docker build -t k8sapp_ui_template .",
    "docker:run": "docker run --rm -it -p 3000:3000 k8sapp_ui_template",
    "test": "jest src --verbose",
    "test:ci": "jest src --ci",
    "test:links": "start-server-and-test start http://localhost:3000 \"blc --input http://localhost:3000 --filter-level 3 -ro --exclude /pages/dynamic-sitemap.xml --verbose \"",
    "test:size": "size-limit",
    "lint:js": "eslint \"./{src,cypress}/**/*.{js,jsx}\" --fix",
    "lint:styles": "stylelint **/*.scss --fix",
    "format-all": "prettier --write .",
    "cy:run": "yarn cypress run",
    "cy:open": "yarn cypress open",
    "cy:ci": "start-server-and-test start http://localhost:3000 cy:run",
    "cy:headless": "yarn build && start-server-and-test start http://localhost:3000 cy:run",
    "cy:ui": "start-server-and-test dev http://localhost:3000 cy:open",
    "storybook:build": "build-storybook",
    "storybook:run": "start-storybook -p 6006",
    "preflight": "yarn browserslist --update-db && yarn lint:js && yarn lint:styles && yarn format-all && yarn test -u && yarn cy:ci && yarn test:size"
  },
  "dependencies": {
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@sentry/integrations": "^6.0.3",
    "@sentry/nextjs": "^6.13.3",
    "@sentry/webpack-plugin": "^1.18.3",
    "@storybook/addon-actions": "^6.5.0-alpha.46",
    "@storybook/addon-essentials": "^6.5.0-alpha.46",
    "@storybook/addon-links": "^6.5.0-alpha.46",
    "@storybook/react": "^6.5.0-alpha.46",
    "babel-plugin-dynamic-import-node": "^2.3.3",
    "beautiful-react-hooks": "^0.35.0",
    "bootstrap": "^5.0.0",
    "broken-link-checker": "^0.7.8",
    "classnames": "^2.3.1",
    "console-stamp": "^3.0.3",
    "is_js": "^0.9.0",
    "next": "11",
    "next-auth": "^3.27.3",
    "next-seo": "^4.26.0",
    "next-sitemap": "^1.6.203",
    "nextjs-sitemap-generator": "^1.3.1",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-bootstrap-icons": "1.0.0",
    "react-device-detect": "^2.0.1",
    "react-dom": "^17.0.2",
    "react-gtm-module": "^2.0.11",
    "react-html-parser": "^2.0.2",
    "react-responsive": "^8.2.0",
    "sass": "1.32.0",
    "sass-loader": "10.1.0",
    "swr": "1.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@size-limit/file": "^7.0.8",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/builder-webpack5": "^6.4.19",
    "@storybook/manager-webpack5": "^6.4.19",
    "@svgr/webpack": "^5.5.0",
    "@testing-library/cypress": "8.0.2",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^11.1.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.5.2",
    "babel-loader": "^8.2.2",
    "chromatic": "^5.10.1",
    "cypress": "8.3.0",
    "cypress-wait-until": "^1.7.2",
    "eslint": "^7.22.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-chai-friendly": "0.6.0",
    "eslint-plugin-cypress": "2.11.2",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-storybook": "^0.5.7",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.5.3",
    "mockdate": "^3.0.2",
    "prettier": "^2.1.2",
    "react-test-renderer": "^17.0.1",
    "size-limit": "^7.0.8",
    "start-server-and-test": "1.12.0",
    "storybook-addon-next": "^1.6.1",
    "stylelint": "^13.12.0",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-recommended-scss": "^4.2.0",
    "stylelint-no-unsupported-browser-features": "^4.1.4",
    "stylelint-scss": "^3.19.0"
  }
}
