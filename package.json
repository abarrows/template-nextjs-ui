{
  "name": "template-nextjs-ui",
  "description": "This is the GitHub template repo that serves as a single source of truth for synced React.js/Next.js UIs",
  "author": "Andy Barrows",
  "homepage": "https://github.com/abarrows/template-nextjs-ui",
  "version": "0.0.0",
  "license": "UNLICENSED",
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/abarrows/template-nextjs-ui"
  },
  "scripts": {
    "build": "next build",
    "build:analyze": "ANALYZE=true npm run build",
    "dev": "next dev",
    "docker:build": "docker compose up --build",
    "docker:run": "docker compose up",
    "format": "prettier --write .",
    "lint:container": "npx mega-linter-runner",
    "lint:js": "eslint \"./{src,playwright}/**/*.{js,jsx}\" --fix",
    "lint:js:check": "npx eslint-config-prettier ${1}",
    "lint:js:rules": "eslint --print-config .eslintrc.js",
    "lint:styles": "stylelint **/*.scss --fix",
    "lint:styles:check": "stylelint-config-prettier-scss-check",
    "lint:styles:nofix": "stylelint **/*.scss",
    "lint:styles:rules": "stylelint --print-config .",
    "preflight": "npm run browserslist --update-db && npm run lint:js && npm run lint:styles && npm run format && npm run test:unit:ci && npm run build && npm run test:integration:ci && npm run test:size && npm run test:integration:report",
    "pretest:integration:ci": "npx playwright install --with-deps",
    "setup": "yarn install && yarn keys:setup ${1:-development}",
    "start": "next start",
    "storybook:build": "storybook build",
    "storybook:dev": "storybook dev",
    "test:integration": "playwright test --trace on",
    "test:integration:ci": "CI=true playwright test",
    "test:integration:record": "playwright codegen",
    "test:integration:report": "playwright show-report",
    "test:size": "size-limit",
    "test:unit": "jest src --verbose",
    "test:unit:ci": "jest src --ci",
    "test:unit:watch": "jest --watch --detectOpenHandles"
  },
  "dependencies": {
    "@next/bundle-analyzer": "^13.4.19",
    "classnames": "^2.3.2",
    "console-stamp": "^3.1.1",
    "next": "13.4.6",
    "prop-types": "^15.8.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "sass": "^1.62.1"
  },
  "devDependencies": {
    "@babel/core": "^7.21.8",
    "@babel/eslint-parser": "^7.21.8",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-react": "^7.18.6",
    "@playwright/test": "^1.34.3",
    "@size-limit/file": "^8.2.4",
    "@storybook/addon-a11y": "^7.0.17",
    "@storybook/addon-essentials": "^7.0.17",
    "@storybook/addon-interactions": "^7.0.17",
    "@storybook/addon-links": "^7.0.17",
    "@storybook/blocks": "^7.0.17",
    "@storybook/nextjs": "^7.0.17",
    "@storybook/react": "^7.0.17",
    "@storybook/testing-library": "^0.0.14-next.2",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@trivago/prettier-plugin-sort-imports": "^4.1.1",
    "babel-jest": "^29.5.0",
    "babel-loader": "^9.1.2",
    "babel-plugin-dynamic-import-node": "^2.3.3",
    "eslint": "8.41.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-next": "13.4.3",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-chai-friendly": "^0.7.2",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-storybook": "^0.6.12",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "jest-fetch-mock": "^3.0.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.8",
    "size-limit": "^8.2.4",
    "storybook": "^7.0.17",
    "stylelint": "^15.6.2",
    "stylelint-color-format": "^1.1.0",
    "stylelint-config-prettier-scss": "^1.0.0",
    "stylelint-config-recess-order": "^4.0.0",
    "stylelint-config-standard-scss": "^9.0.0",
    "stylelint-no-unsupported-browser-features": "^6.1.0"
  }
}
