{
  "name": "template-nextjs-ui",
  "description": "This is the GitHub template repo that serves as a single source of truth for synced React.js/Next.js UIs",
  "author": "Andy Barrows",
  "homepage": "https://github.com/abarrows/template-nextjs-ui",
  "version": "0.0.0",
  "license": "UNLICENSED",
  "repository": {
    "type": "git",
    "url": "https://github.com/abarrows/template-nextjs-ui"
  },
  "scripts": {
    "build": "next build && yarn build:sitemap",
    "build:analyze": "ANALYZE=true yarn build",
    "build:sitemap": "next-sitemap --config next-sitemap.config.js",
    "dev": "next dev",
    "format": "prettier . --list-different --write",
    "format-all": "prettier . --list-different --write",
    "keys:edit": "pwsh -c \"./Edit-Secrets.ps1\"",
    "keys:get": "pwsh -c \"./Get-Secrets.ps1 -Environment ${@}\"",
    "keys:save": "pwsh -c \"./Set-Secrets.ps1\"",
    "lint:js": "eslint \"./{src,playwright}/**/*.{js,jsx}\" --fix",
    "lint:js:rules": "eslint --print-config .eslintrc.js",
    "lint:styles": "stylelint --fix **/*.{scss,css}",
    "lint:styles:rules": "stylelint --print-config .",
    "setup": "corepack enable && yarn install && yarn plugin import interactive-tools && yarn run keys:get ${0:-development}",
    "start": "next start -p 3000",
    "storybook:build": "storybook build",
    "storybook:deploy": "chromatic --auto-accept-changes --exit-zero-on-changes --build-script-name storybook:build",
    "storybook:run": "storybook dev -p 6006",
    "test:dependencies": "npm-check ./ --specials=bin,nextjs,react,webpack,jest,babel,eslint,prettier,stylelint,typescript,node,npm,yarn",
    "test:duplication:ci": "jscpd . && open coverage/code-quality/duplication/html/index.html",
    "test:integration": "yarn test:integration:setup && playwright test --trace on",
    "test:integration:ci": "CI=true yarn test:integration:setup && playwright test --trace on",
    "test:integration:debug": "yarn run playwright test ${0:-.} --debug --trace on",
    "test:integration:record": "yarn run playwright codegen",
    "test:integration:report": "playwright show-report",
    "test:integration:setup": "yarn run playwright install",
    "test:size": "size-limit",
    "test:storybook": "test-storybook",
    "test:unit": "jest ${0:-.} --verbose",
    "test:unit:ci": "jest ${0:-.} --ci",
    "test:unit:watch": "jest --watch --detectOpenHandles",
    "upgrade:dependencies": "yarn upgrade-interactive",
    "validate:ci": "mega-linter-runner --flavor cupcake --release latest --fix",
    "validate": "npm-run-all \"lint:*\" format test:unit:ci test:integration:ci test:size test:integration:report"
  },
  "dependencies": {
    "@next/bundle-analyzer": "^13.5.6",
    "axe-playwright": "^2.0.1",
    "classnames": "^2.5.1",
    "consola": "^3.2.3",
    "date-fns": "^3.6.0",
    "next": "^14.1.4",
    "next-auth": "^4.24.7",
    "next-sitemap": "^4.2.3",
    "prop-types": "^15.8.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-gtm-module": "^2.0.11",
    "sass": "^1.72.0",
    "sharp": "^0.33.3"
  },
  "devDependencies": {
    "@axe-core/react": "^4.8.2",
    "@babel/core": "^7.21.8",
    "@babel/eslint-parser": "^7.23.3",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-react": "^7.23.3",
    "@ianvs/prettier-plugin-sort-imports": "^4.1.1",
    "@jscpd/badge-reporter": "^3.4.5",
    "@lhci/cli": "^0.13.0",
    "@playwright/test": "^1.34.3",
    "@size-limit/file": "^8.2.4",
    "@storybook/addon-a11y": "^8.0.4",
    "@storybook/addon-essentials": "^8.0.4",
    "@storybook/addon-interactions": "^8.0.4",
    "@storybook/addon-links": "^8.0.4",
    "@storybook/blocks": "^8.0.4",
    "@storybook/nextjs": "^8.0.4",
    "@storybook/react": "^8.0.4",
    "@storybook/test-runner": "^0.17.0",
    "@storybook/testing-library": "^0.2.2",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.2",
    "@testing-library/react-hooks": "^8.0.1",
    "@typescript-eslint/eslint-plugin": "^7.4.0",
    "@typescript-eslint/parser": "^7.4.0",
    "babel-jest": "^29.7.0",
    "babel-loader": "^9.1.3",
    "babel-plugin-dynamic-import-node": "^2.3.3",
    "chromatic": "^11.2.0",
    "eslint": "^8.57.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-next": "^14.1.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-chai-friendly": "^0.7.4",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jest": "^27.9.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-storybook": "^0.8.0",
    "eslint-plugin-unused-imports": "^3.1.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-fetch-mock": "^3.0.3",
    "jscpd": "^3.5.10",
    "npm-check": "^6.0.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.38",
    "prettier": "^3.2.5",
    "size-limit": "^11.1.2",
    "storybook": "^8.0.4",
    "stylelint": "^16.3.1",
    "stylelint-color-format": "^1.1.0",
    "stylelint-config-prettier-scss": "^1.0.0",
    "stylelint-config-recess-order": "^5.0.0",
    "stylelint-config-standard-scss": "^13.0.0",
    "stylelint-no-unsupported-browser-features": "^8.0.1",
    "typescript": "^5.4.3"
  },
  "packageManager": "yarn@3.6.1"
}
