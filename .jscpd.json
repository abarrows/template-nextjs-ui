{
  "blame": true,
  "exitCode": 1,
  "gitIgnore": true,
  "ignore": [
    "**/node_modules/**",
    "**/.git/**",
    "**/.rbenv/**",
    "**/.venv/**",
    "**/*cache*/**",
    "**/.github/**",
    "**/.idea/**",
    "**/report/**",
    "**/*.svg"
  ],
  "output": "coverage/code-quality/duplication/",
  "reporters": ["html", "markdown", "badge", "console", "json-summary"],
  "threshold": 15
}
